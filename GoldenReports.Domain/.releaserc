{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": "next"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": ["dist/*.nupkg"]
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "dotnet pack GoldenReports.Domain -c Release --include-symbols /p:AssemblyVersion=${nextRelease.version} /p:Version=${nextRelease.version} --output /dist"
      }
    ],
  ]
}