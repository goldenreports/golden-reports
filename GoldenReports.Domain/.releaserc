{
  "extends": "semantic-release-monorepo",
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": "next"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "dotnet pack $PROJECT_PATH -c Release --include-symbols /p:AssemblyVersion=${nextRelease.version} /p:Version=${nextRelease.version} --output $PACKAGE_OUTPUT_DIRECTORY",
        "publishCmd": "dotnet nuget push $PACKAGE_OUTPUT_DIRECTORY/*.nupkg -k $NUGET_TOKEN -s $NUGET_PUSH_URL"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": ["dist/*.nupkg"]
      }
    ]
  ]
}